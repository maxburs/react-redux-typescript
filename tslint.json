{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-immutable",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-var-keyword": true,
    "no-parameter-reassignment": true,
    "typedef": [true],
    "readonly-keyword": true,
    "readonly-array": true,
    "no-let": true,
    "no-object-mutation": true,
    "no-delete": true,
    "no-method-signature": true,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "curly": false
  }
}
